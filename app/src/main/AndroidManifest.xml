<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.whartonsummit.android_app.pwcs_android">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/NavAppTheme">
        <activity android:name=".Activities.ScheduleActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.SpeakersActivity" />
        <activity android:name=".Activities.MoreActivity" />
        <activity android:name=".Activities.MainActivity" />
        <activity android:name=".Activities.PanelDetailsActivity" />
        <activity android:name=".Activities.SpeakerDetailsActivity"></activity>
    </application>

</manifest>